{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api-proxy/:path*",
      "destination": "https://digitalreach.app/:path*"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://digitalreach.app/api/public/sites/3d6c918d-5c39-47e9-8109-be79b2e8c302/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
